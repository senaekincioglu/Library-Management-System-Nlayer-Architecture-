@model Library_Management_System_DAL.Entity.Employee
@{
    ViewBag.Title = "BringEmployee";
    Layout = "~/Views/Shared/Layout.cshtml";
}


@using (Html.BeginForm("UpdateEmployee", "Employee", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(k => k.Id)
        @Html.TextBoxFor(k => k.Id, new { @class = "form-control" })
        <br />

        <label>PERSONEL ADI</label>
        @Html.TextBoxFor(k => k.Employees, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>
}
